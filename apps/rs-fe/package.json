{
  "name": "whistler-dashboard-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "PRODUCTION=false npm run config && ng serve --host 0.0.0.0",
    "start:mock": "PRODUCTION=false MOCK=true npm run config && ng serve --host 0.0.0.0",
    "start:prod": "PRODUCTION=true npm run config && ng serve --aot --prod --host 0.0.0.0",
    "build": "PRODUCTION=false npm run config && ng build",
    "build:prod": "PRODUCTION=true npm run config && ng build --aot --prod",
    "config": "ts-node set-env.ts",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precommit": "node ./scripts/precommit.js && git add index.ts src/library/open-ui-components/index.ts src/library/open-services/index.ts ",
    "postinstall": "ts-node set-env.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular2-jsonapi": "^7.1.0",
    "core-js": "^2.5.4",
    "luxon": "^1.12.1",
    "ngx-auth": "^5.0.0",
    "ngx-file-drop": "^8.0.2",
    "ngx-material-timepicker": "^3.0.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.7",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "dotenv": "^7.0.0",
    "husky": "^2.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.12.0",
    "protractor": "~5.4.0",
    "rxjs-tslint": "0.1.5",
    "rxjs-tslint-rules": "^4.24.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}

{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "console-app": {
      "root": "src/projects/console-app/",
      "sourceRoot": "src/projects/console-app",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/console-app",
            "index": "src/projects/console-app/index.html",
            "main": "src/projects/console-app/main.ts",
            "polyfills": "src/projects/polyfills.ts",
            "tsConfig": "src/projects/tsconfig.app.json",
            "assets": [
              "src/projects/console-app/favicon.ico",
              "src/projects/console-app/assets"
            ],
            "styles": [
              "src/projects/console-app/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "console-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "console-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "console-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/projects/test.ts",
            "polyfills": "src/projects/polyfills.ts",
            "tsConfig": "src/projects/tsconfig.spec.json",
            "karmaConfig": "src/projects/karma.conf.js",
            "styles": [
              "src/projects/console-app/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/projects/console-app/favicon.ico",
              "src/projects/console-app/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/projects/tsconfig.app.json",
              "src/projects/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "console-app-e2e": {
      "root": "e2e/console-app/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/console-app/protractor.conf.js",
            "devServerTarget": "console-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "console-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/console-app/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "whistler-iam": {
      "root": "src/projects/whistler/iam",
      "sourceRoot": "src/projects/whistler/iam/src",
      "projectType": "library",
      "prefix": "iam",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "src/projects/whistler/iam/tsconfig.lib.json",
            "project": "src/projects/whistler/iam/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/projects/whistler/iam/src/test.ts",
            "tsConfig": "src/projects/whistler/iam/tsconfig.spec.json",
            "karmaConfig": "src/projects/whistler/iam/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/projects/whistler/iam/tsconfig.lib.json",
              "src/projects/whistler/iam/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "whistler-cognito": {
      "root": "src/projects/whistler/cognito",
      "sourceRoot": "src/projects/whistler/cognito/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "src/projects/whistler/cognito/tsconfig.lib.json",
            "project": "src/projects/whistler/cognito/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/projects/whistler/cognito/src/test.ts",
            "tsConfig": "src/projects/whistler/cognito/tsconfig.spec.json",
            "karmaConfig": "src/projects/whistler/cognito/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/projects/whistler/cognito/tsconfig.lib.json",
              "src/projects/whistler/cognito/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "console-app",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}
